# .env 파일

#Kafka 설정
KAFKA_BROKER=kafka:29092 #로컬 환경 KAFKA 주소
#KAFKA_BROKER=100.113.118.32:9092 #tailscale 맥 KAFKA 주소
KAFKA_TOPIC_RAW=topic_raw
KAFKA_TOPIC_SEARCH=search-queue
KAFKA_TOPIC_AI=ai-queue

#크롤러 설정
TARGET_URL=
CRAWLER_TIMEOUT=10
CRAWLER_RETRY_ALT_UA=true
MAX_CRAWL_RETRIES=3
CRAWL_DELAY_SECONDS=2

#vLLM 설정 (AI Worker)
# RTX 4070 (12GB VRAM) 최적화: AWQ 4-bit 양자화 모델 사용
VLLM_MODEL=Qwen/Qwen2.5-7B-Instruct-AWQ
VLLM_QUANTIZATION=awq
VLLM_GPU_MEMORY_UTILIZATION=0.92
VLLM_MAX_MODEL_LEN=6144
VLLM_USE_V1=0  # v0 API 사용 (v1은 실험적, 불안정)

# Database
DATABASE_URL=postgresql://agent:agent_password@postgres:5432/ai_agent

#키
SEARCH_ENGINE_API_KEY=
OPENAI_API_KEY=